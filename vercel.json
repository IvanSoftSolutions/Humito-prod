{
    "version": 2,
    "builds": [
      {
        "src": "humito_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
      }
    ],
    "routes": [
      {
        "src": "/static/humito_site/(.*)",
        "dest": "/static/humito_site/$1"
      },
      {
        "src": "/(.*)",
        "dest": "humito_project/wsgi.py"
      }
    ]
  }